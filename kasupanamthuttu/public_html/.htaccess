RewriteEngine On
AddType video/mp4 .mp4

RewriteRule ^([a-zA-Z0-9@_-]+)$ index.php?wpage=$1 [QSA]
RewriteRule ^@([a-zA-Z0-9_-]+)$ index.php?wpage=@$1                     
RewriteRule ^viewProduct/(.*)$ index.php?wpage=viewProduct&param=$1     
RewriteRule ^EditProduct/(.*)$ index.php?wpage=EditProduct&param=$1     
RewriteRule ^viewproduct/(.*)$ index.php?wpage=viewproduct&param=$1     

RewriteRule ^M2W_viewItem/(.*)$ index.php?wpage=M2W_viewItem&param=$1    [QSL] 
RewriteRule ^M2W_EditItem/(.*)$ index.php?wpage=M2W_EditItem&param=$1   [QSL]

RewriteRule ^BBA_viewItem/(.*)$ index.php?wpage=BBA_viewItem&param=$1    [QSL] 
RewriteRule ^BBA_EditItem/(.*)$ index.php?wpage=BBA_EditItem&param=$1 [QSL]

RewriteRule ^S2W_viewItem/(.*)$ index.php?wpage=S2W_viewItem&param=$1    [QSL] 
RewriteRule ^S2W_EditItem/(.*)$ index.php?wpage=S2W_EditItem&param=$1 [QSL]

# php -- BEGIN cPanel-generated handler, do not edit
